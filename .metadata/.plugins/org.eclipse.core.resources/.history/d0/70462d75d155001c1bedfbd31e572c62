package codechef;
import java.util.*;
public class ReverseTheNumber {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Number of Test cases => ");
		int t=sc.nextInt();
		for(int i=0;i<t;i++) {
			int n=sc.nextInt();
			int r,temp,m=n;
			int x,r1=0;
			do {
				r=m%10;
				
				temp=r;
				m=m/10;
				x=r1*10+r;
				r1=r;
			}while(r!=0);
			System.out.println(x);
		}
	}

}
