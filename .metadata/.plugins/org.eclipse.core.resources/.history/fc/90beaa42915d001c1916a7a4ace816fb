package codechef;
import java.util.*;
public class TheLeadGame {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Give number of lines => ");
		int n=sc.nextInt();
		int p1[]=new int[n+1];
		int p2[]=new int[n+1];
		p1[0]=0;p2[0]=0;
		int CfP1=0,CfP2=0;
		
		for(int i=1;i<=n;i++) {
			p1[i]=sc.nextInt();
			p2[i]=sc.nextInt();
			CfP1=p1[i-1]+p1[i];
			System.out.print(CfP1+" ");
			CfP2=p2[i-1]+p2[i];
			System.out.println(CfP2);
		}

	}

}
